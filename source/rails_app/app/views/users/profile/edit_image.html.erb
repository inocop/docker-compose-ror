<h2>Edit Icon</h2>
<%= form_for(@user, url: users_profile_edit_image_url, html: {method: :post, multipart: true}) do |form| %>

  <%= render(partial: "view_parts/form/input_file", locals: {form: form, field_name: :user_image_path}) %>

  <div class="actions">
    <%= form.submit "Update", :class => "btn btn-primary" %>
    <%= link_to "Back", 'javascript:void(0);', onclick: "history.back();", :class => "ml-3 btn btn-primary" %>
  </div>
<% end %>